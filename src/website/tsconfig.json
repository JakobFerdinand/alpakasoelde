{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "preserve",
    "jsxImportSource": "astro",
    "types": [
      "astro/client",
      "astro/astro-jsx",
      "astro/astro-jsx-runtime",
      "@playwright/test",
      "@types/node"
    ],
    "plugins": [
      {
        "name": "astro/ts-plugin"
      }
    ]
  }
}